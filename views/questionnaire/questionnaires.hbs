<div>
    <h3 class="text-center">Lista Questionari</h3>
    {{#each users}}
<div class="table-responsive mx-auto">

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Utente</th>
        <th>Questionario</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>{{username}}</td>
        <td>{{type_description}}</td>
        <td>
          <a href="/questionnaires/questionnaire?user_id={{id}}&questions_type_id={{questions_type_id}}" class="btn btn-primary">Vai al Questionario</a>
        </td>
      </tr>
    </tbody>
  </table>
</div>
   {{/each}}
</div>
